public Integer nextPrime(Integer num) {
    //code here
 Boolean isprime;
{
    num=num+1;
    isprime=isprime(num);
}while (!isPrime)

return num;
    
}

public static Boolean isPrime(Integer num) {
  
    for (Integer i=2;i<num;i++){
        if (Math.mod(num, i)==0){
            return false;
        }
    }
    return true;
}