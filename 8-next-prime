public Integer nextPrime(Integer num) {
    //code here
{
    num=num+1;
    Boolean isprime=isprime(num);
}while (!isPrime)
return num;
    
}

public Boolean isPrime(Integer num) {
  
    for (Integer i=2;i<num;i++){
        if (Math.mod(num, i)==0){
            return false;
        }
    }
    return true;
}