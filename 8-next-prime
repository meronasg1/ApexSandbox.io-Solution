public Integer nextPrime(Integer num) {
    //code here
 Boolean isprime= false;
  if(num < 2) {

        return 2;

    }
      Integer  nums =num+1;

while (!isPrime){
    
    isprime=isprime(nums);
    num=num+1;
}

return num;
    
}

public static Boolean isPrime(Integer num) {
  
    for (Integer i=2;i<num;i++){
        if (Math.mod(num, i)==0){
            return false;
        }
    }
    return true;
}