public Boolean isLeapYear(Integer year) {
    //code here
Integer divBy4 = Math.mod(year, 4);
Integer divBy100 = Math.mod(year, 100);
Integer divBy400 = Math.mod(year, 400);

if (divBy4==0){
    return false;
}else if(divBy100==0) {
    return false;
}else if(divBy400==0) {
    return false;
}else{
    return true;
}
}