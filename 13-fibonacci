public Integer fibonacci(Integer n) {
    //code here
    Integer lastValue = 1;
    Integer currentValue = 1;

    if (n==1||n==2){

       return currentValue;
    }
    else{
            for (Integer i=2; i<n; i++){
            currentValue =currentValue+lastValue;
                        lastValue=currentValue;


            }
                        return currentValue;

    }
    
}