public Integer fibonacci(Integer n) {
    //code here
    Integer lastValue = 1;
    Integer currentValue = 1;
    Integer sum = lastValue+currentValue;
    if (n==1||n==2){

       return currentValue;
    }
    
    for (Integer i=3; i<n; i++){

        lastValue =currentValue;
        currentValue=sum;
        sum = lastValue+currentValue;

    }
    return sum;
                        

    
    
}