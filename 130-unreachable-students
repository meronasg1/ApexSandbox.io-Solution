public List<apxio__Student__c> selectUnreachableStudents() {
    // code here
      List <apxio__Student__c> thisStudent =[ Select id,Name, apxio__Active__c from apxio__Student__c where apxio__Active__c   = true && apxio__Phone__c= null && apxio__Email__c = null ASC Name 
]; 
